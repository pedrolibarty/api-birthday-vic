{
  "version": 2,
  "builds": [{ "src": "api/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/guests", "dest": "/api/listGuests.js" },
    { "src": "/api/companions", "dest": "/api/listCompanions.js" },
    { "src": "/api/companions/:id_guest", "dest": "/api/updateCompanions.js" }
  ]
}
